{
  "name": "test-cq-build",
  "displayName": "Test CQ Build",
  "description": "Build",
  "version": "0.0.1",
  "repository": "https://github.com/adarsha45/vscode-ext",
  "publisher": "CQDevops",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:test-cq-build.buildtrigger"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "test-cq-build.buildtrigger",
        "title": "Test the function",
        "category": "CQ"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.45",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "jenkins": "0.28.1"
  },
  "dependencies": {
    "jenkins": "^0.28.1"
  }
}
